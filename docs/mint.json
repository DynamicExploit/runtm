            ]
          }
        ]
      },
      {
        "tab": "CLI",
        "icon": "terminal",
        "groups": [
          {
            "group": "CLI Reference",
            "pages": [
              "cli/overview",
              "cli/auth",
              "cli/init",
              "cli/run",
              "cli/deploy",
              "cli/validate",
              "cli/fix",
              "cli/status",
              "cli/logs",
              "cli/list",
              "cli/search",
              "cli/destroy",
              "cli/secrets",
              "cli/domain",
              "cli/config",
              "cli/approve"
            ]
          }
        ]
      },
      {
        "tab": "Self-Hosting",
        "icon": "server",
        "groups": [
          {
            "group": "Self-Hosting",
            "pages": [
              "self-hosting/overview",
