      "label": "Get Started",
      "href": "https://app.runtm.com"
    }
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "icon": "book-open",
        "groups": [
          {
            "group": "Get Started",
            "pages": [
              "introduction",
              "quickstart"
            ]
          },
          {
            "group": "Templates",
            "pages": [
              "templates/overview",
              "templates/backend-service",
              "templates/static-site",
              "templates/web-app"
            ]
          },
          {
            "group": "Features",
            "pages": [
              "features/machine-tiers",
              "features/secrets",
              "features/database",
              "features/auth",
              "features/custom-domains"
            ]
          },
          {
            "group": "Guides",
            "pages": [
              "guides/agent-workflow",
              "guides/discovery",
              "guides/logs-debugging",
              "guides/troubleshooting"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "icon": "code",
        "groups": [
          {
            "group": "Overview",
            "pages": [
              "api/overview"
            ]
          },
          {
            "group": "Deployments",
            "pages": [
              "api/deployments/list",
              "api/deployments/create",
              "api/deployments/get",
              "api/deployments/delete",
              "api/deployments/search"
            ]
          },
          {
            "group": "Logs",
            "pages": [
              "api/logs"
            ]
          },
          {
            "group": "Domains",
            "pages": [
              "api/domains/add",
              "api/domains/get",
              "api/domains/remove"
            ]
          },
          {
            "group": "User",
            "pages": [
