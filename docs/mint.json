{
  "$schema": "https://mintlify.com/docs.json",
  "name": "Runtm",
  "logo": {
    "dark": "/images/logo-dark.svg",
    "light": "/images/logo-light.svg"
  },
  "favicon": "/images/favicon.svg",
  "colors": {
    "primary": "#18181b",
    "light": "#71717a",
    "dark": "#18181b",
    "anchors": {
      "from": "#18181b",
      "to": "#3f3f46"
    },
    "background": {
      "light": "#fafafa",
      "dark": "#18181b"
    }
  },
  "theme": "quill",
  "modeToggle": {
    "default": "light"
  },
  "navbar": {
    "links": [
      {
        "label": "Dashboard",
        "href": "https://app.runtm.com"
      }
    ],
    "primary": {
